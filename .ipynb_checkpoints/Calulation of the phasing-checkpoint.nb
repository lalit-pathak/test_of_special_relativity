       SubscriptBox["v", "e"], 
       RowBox[{"Sin", "[", "i", "]"}]}], ")"}]}], 
    RowBox[{
     RowBox[{"Sqrt", "[", "2", "]"}], "\[Pi]", " ", "c"}]], 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox["x", "2"]}], "+", 
       FractionBox[
        SuperscriptBox["x", "6"], "20"], "-", 
       FractionBox[
        SuperscriptBox["x", "10"], "72"], "+", 
       RowBox[{"5", 
